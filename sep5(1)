#include<stdio.h>
void main()
{
    int n,m;
    printf("enter n and m values");
    scanf("%d %d",&n,&m);
    display(n,m);
}
void display(int n,int m)
{
    if(n>m)
    return 1;
    else
    {
        printf("%d\n",n);
        display(n+1,m);
    }
}
output:
enter n and m values 2 10
2
3
4
5
6
7
8
9
10
