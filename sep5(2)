include<stdio.h>
int main()
{
    int n,i,a[10];
    printf("enter array size");
    scanf("%d",&n);
    i=0;
    while(i<=n)
    {
        scanf("%d",&a[i]);
        printf("%d\t",a[i]);
        i++;
    }
    printf("%d\t",a[i]);
    return 0;
}
output:
enter array size 5
enetr elements into array 1 2 3 4 5
12345
