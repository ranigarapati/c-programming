#include<stdio.h>
void main()
{
    int n,temp;
    printf("enter a number");
    scanf("%d",&n);
    temp=fact(n);
    printf("factorial of %d=%d",n,temp);
}
int fact(int n)
{
    int i=1,sum=1;
    while(i<=n)
    {
        sum=sum*i;
        i++;
    }
    return(sum);
 }
 enter a number
 5
 factorial of 5=120
      
