#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i,n,sum=0,*p;
    printf("enter the number of elements");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",p+i);
        sum=sum+ *(p+i);
    }
    printf("sum is %d\n",sum);
    return 0;
    }
enter number of elements:4
19 21 10 9
sum is 59
